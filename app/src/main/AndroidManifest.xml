<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apple.keyboarddemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".keyboard_num.KeyBoardNumberActivity"
            android:label="数字软键盘"></activity>
        <activity
            android:name=".SplashActivity"
            android:label="软键盘项目">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".topic.TopicActivity"
            android:label="话题"
            android:theme="@style/topic" />
        <activity
            android:name=".shelter_aty.ShelterActivity"
            android:windowSoftInputMode="adjustPan"/>
    </application>

</manifest>